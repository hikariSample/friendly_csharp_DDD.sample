<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="renderer" content="webkit">

    <title>@ViewData["Title"] 平台方管理系统</title>

    <meta name="keywords" content="H+后台主题,后台bootstrap框架,会员中心主题,后台HTML,响应式后台">
    <meta name="description" content="H+是一个完全响应式，基于Bootstrap3最新版本开发的扁平化主题，她采用了主流的左右两栏式布局，使用了Html5+CSS3等现代技术">

    <!--[if lt IE 9]>
        <meta http-equiv="refresh" content="0;ie.html" />
    <![endif]-->

    <link rel="~/shortcut icon" href="favicon.ico">
    <link href="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="https://use.fontawesome.com/releases/v5.7.2/css/all.css" rel="stylesheet">
    <link href="https://cdn.staticfile.org/iCheck/1.0.2/skins/all.css" rel="stylesheet">
    <link href="https://cdn.staticfile.org/animate.css/3.7.0/animate.min.css" rel="stylesheet">
    <link href="~/css/style.css?v=4.1.0" rel="stylesheet">

    <link href="https://unpkg.com/bootstrap-table@1.14.1/dist/bootstrap-table.min.css" rel="stylesheet">
    <link href="~/lib/Layx-2.5.4/layx.min.css" rel="stylesheet" type="text/css" />

    <style type="text/css">
        .input_xs {
            width: 100px;
        }

        .ma_sm {
            margin: 0 7px;
        }

        .fixed-table-container {
            height: 485px !important;
        }
    </style>
    @RenderSection("Css", required: false)
</head>
<body class="gray-bg">
    @RenderBody()
    <!-- 全局js -->
    <script src="https://cdn.staticfile.org/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.6/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <script src="https://cdn.staticfile.org/jquery.form/4.2.2/jquery.form.min.js"></script>
    <script src="https://cdn.staticfile.org/bootstrap-show-password/1.1.2/bootstrap-show-password.min.js"></script>

    @await Html.PartialAsync("_ValidationScriptsPartial")
    <!-- 自定义js -->
    <script src="~/js/content.js?v=1.0.0"></script>

    <!-- Bootstrap table -->
    <script src="https://unpkg.com/bootstrap-table@1.14.1/dist/bootstrap-table.min.js"></script>
    @*<script src="~/lib/bootstrap-table/extensions/mobile/bootstrap-table-mobile.min.js?v=1.1.41"></script>*@
    <script src="https://cdn.staticfile.org/bootstrap-table/1.14.2/locale/bootstrap-table-zh-CN.min.js"></script>

    <!-- Data picker -->
    <script src="https://cdn.staticfile.org/bootstrap-datepicker/1.8.0/js/bootstrap-datepicker.min.js"></script>

    <!-- iCheck -->
    <script src="https://cdn.staticfile.org/iCheck/1.0.2/icheck.min.js"></script>
    <script>
        $(document).ready(function () {
            $('.i-checks').iCheck({
                checkboxClass: 'icheckbox_square-green',
                radioClass: 'iradio_square-green',
            });
        });
    </script>

    <!-- 第三方插件 -->
    <script src="~/lib/Layx-2.5.4/layx.min.js" type="text/javascript"></script>

    <script type="text/javascript" src="~/js/site.js"></script>
    <!--请在下方写此页面业务相关的脚本-->
    @RenderSection("Scripts", required: false)
    <!--/请在上方写此页面业务相关的脚本-->
</body>
</html>